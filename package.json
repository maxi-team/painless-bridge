{
  "name": "@mntm/painless-bridge",
  "version": "5.1.1",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./lib/index.mjs"
      },
      "default": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./lib/plugins": {
      "import": {
        "types": "./lib/plugins/index.d.mts",
        "default": "./lib/plugins.mjs"
      },
      "default": {
        "types": "./lib/plugins/index.d.ts",
        "default": "./lib/plugins.js"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "author": {
    "name": "Anton Petrov",
    "email": "im@petrov.engineer",
    "url": "https://petrov.engineer"
  },
  "description": "A package for integrating VK Mini Apps with official VK clients for iOS, Android and Web",
  "license": "MIT",
  "keywords": [
    "vk",
    "vkontakte",
    "vkminiapps",
    "vk-mini-apps",
    "bridge",
    "connect",
    "vk-api",
    "vk-apps",
    "vk-connect",
    "vk-bridge"
  ],
  "homepage": "https://github.com/maxi-team/painless-bridge",
  "bugs": "https://github.com/maxi-team/painless-bridge/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxi-team/painless-bridge.git"
  },
  "scripts": {
    "build": "vite build"
  },
  "devDependencies": {
    "@mntm/eslint-config": "^2.0.0",
    "@types/node": "^20.10.4",
    "eslint": "^8.55.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vite-plugin-dts": "^3.6.4"
  },
  "resolutions": {
    "esbuild": "^0.19.8"
  }
}
